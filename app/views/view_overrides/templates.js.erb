var all_templates = new Array();
var hook_templates = new Array();
<% @templates.each_with_index do |template,i| %>
  all_templates[<%= i %>] = { id: <%= template.id %> ,name: "<%= template.first %>"};
  <% template[1][:hooks].each do |hook| %>
    hook_templates['<%= hook %>'] = '<%= template.first %>';
  <% end %>
<% end %>

var all_overrides = new Array();
<% ViewOverride.all.each_with_index do |override,i| %>
  all_overrides[<%= i %>] = <%= raw override.to_json %>;
<% end %>

var overrides_by_hook = new Array();
<%- ViewOverride.all.group_by(&:selector).each do |override| -%>
  overrides_by_hook['<%= override[0].gsub("'", "") %>'] = <%= override[1].size %>;
<%- end -%>
